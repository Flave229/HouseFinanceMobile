<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="hoppingvikings.housefinancemobile"
    android:versionCode="4"
    android:versionName="00.03.00">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <service android:name="hoppingvikings.housefinancemobile.WebService.BackgroundService"
            android:exported="false"
            android:stopWithTask="true"/>

        <activity android:name="hoppingvikings.housefinancemobile.UserInterface.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="hoppingvikings.housefinancemobile.UserInterface.DisplayMessageActivity"
            android:label="Display Message">
        </activity>
    </application>

</manifest>